
entity Login {
	usuario String,
    senha Integer
}
entity Arquivista {
    cadlocador String,
    cadlocatario String,
    cadimovel String
}    
 entity Locador  {
     imovel String,
     nome String,
     end String,
     idt String,
     cpf String
 
 }
 entity Locatario {
     imovel String,
     nome String,
     end String,
     idt String,
     cpf String
 }
  
entity Imovel {
    nome String,
    end String,
    contrato String,
    ficha  String,
    oficios String,
    aditivo String
}

entity Pagamento {
    condominio String,
    iptu String,
    luz String,
    agua String,
    diversos String
}

entity Comprovante{
    mes String,
    ano Integer
}

entity Vistoria{
    documentos String,
     imagens String
}

// 1 vistoria Ã© de 1 imovel e 1 imovel pode
// ter N vistorias

// 1 locatario tem n imoveis e 1 imovel
// ten 1 locatario


